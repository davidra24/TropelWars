<application xmlns="http://ns.adobe.com/air/application/25.0">
  <id>com.davidramirez.tropel</id>
  <versionNumber>1.0.0</versionNumber>
  <versionLabel/>
  <filename>Tropel Wars</filename>
  <description/>
  <name>Tropel Wars</name>
  <copyright/>
  <initialWindow>
    <content>Tropel.swf</content>
